{
  "AppSettings": {
    "jwt": {
      "Issuer": "jwt-issuer",
      "Audience": "jwt-audience",
      "SecretKey": "jwt-secret-secure-key"
    },
    "sendGrid": {
      "user": "sendgrid-user-name",
      "key": "sendgrid-apikey"
    },
    "EmailVerficationInfo": {
      "FromName": "frome-name",
      "FromEmail": "frome-email"
    },
    "adminInfo": {
      "username": "admin-username",
      "firstName": "admin-firstName",
      "lastName": "admin-lastName",
      "email": "admin@email.com",
      "password": "admin-password"
    },
    "apphost": "http://angularappurl",
    "appVerPath": "angularAppVerifyEmailPath"
  },
  "ConnectionStrings": {
    "LocalSqlServer": "Server=(localdb)\\mssqllocaldb;Database=EFGetStarted.ConsoleApp.databasename;Trusted_Connection=True;",
    "SqliteConnection": "Data Source=databasename.db"
  },
  "Logging": {
    "IncludeScopes": false,
    "Debug": {
      "LogLevel": {
        "Default": "Warning"
      }
    },
    "Console": {
      "LogLevel": {
        "Default": "Warning"
      }
    }
  }
}